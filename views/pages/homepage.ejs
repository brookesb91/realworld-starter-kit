<div class="container">
  <h1 class="logo-font">Conduit</h1>
  <p> A place to share your <i>Node Js</i> Knowledge</p>
</div>

<div class="container">
  <div class="col-md-9">
    <div class="feed-toggle">
      <div class="nav nav-pills outline-active">

        <li class="nav-item">
          <a class="nav-link" href="#">Your Feed</a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="#">Global Feed</a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="#">TAG_NAME</a>
        </li>
      </div>
      <ul>
      <% articles.forEach(function (article) { %>
        <li><%= article.title %></li>
      <% }); %>
      </ul>
    </div>
  </div>
</div>